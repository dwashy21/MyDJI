<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1.0dip" android:color="#ff1088f2" android:dashGap="0.0dip" />
            <solid android:color="#ff1088f2" />
            <corners android:radius="1.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <stroke android:width="1.0dip" android:color="#d01088f2" android:dashGap="0.0dip" />
            <solid android:color="#d01088f2" />
            <corners android:radius="1.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <stroke android:width="1.0dip" android:color="#401088f2" android:dashGap="0.0dip" />
            <solid android:color="#401088f2" />
            <corners android:radius="1.0dip" />
        </shape>
    </item>
</selector>