<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_20_in_sw320dp" />
            <solid android:color="@color/gs_click_background_color" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_20_in_sw320dp" />
            <solid android:color="#ff3589c5" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_20_in_sw320dp" />
            <solid android:color="#e03589c5" />
        </shape>
    </item>
</selector>