<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:drawable="@drawable/cirsw_track_check" />
    <item>
        <shape android:shape="rectangle"
          xmlns:android="http://schemas.android.com/apk/res/android">
            <size android:height="@dimen/cirsw_height" android:width="@dimen/cirsw_width" />
            <corners android:topLeftRadius="@dimen/cirsw_half_height" android:topRightRadius="@dimen/cirsw_half_height" android:bottomLeftRadius="@dimen/cirsw_half_height" android:bottomRightRadius="@dimen/cirsw_half_height" />
            <solid android:color="#fff2f2f2" />
            <stroke android:width="1.0dip" android:color="@color/cirsw_hover" />
        </shape>
    </item>
</selector>