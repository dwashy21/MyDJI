<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/dp_2" />
            <stroke android:width="1.0dip" android:color="@color/white" />
            <solid android:color="@android:color/transparent" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip android:clipOrientation="horizontal">
            <shape android:shape="rectangle">
                <stroke android:width="@dimen/dp_6" android:color="#00000000" />
                <corners android:radius="@dimen/dp_1" />
                <solid android:color="@color/setting_ui_battery_yellow" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip android:clipOrientation="horizontal">
            <shape android:shape="rectangle">
                <stroke android:width="@dimen/dp_6" android:color="#00000000" />
                <corners android:radius="@dimen/dp_1" />
                <solid android:color="@color/setting_ui_battery_yellow" />
            </shape>
        </clip>
    </item>
</layer-list>