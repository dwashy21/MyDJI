        <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content">
            <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent" android:layout_height="wrap_content">
                <LinearLayout style="@style/setting_ui_rc_stick_ly">
                    <ImageView android:id="@id/setting_ui_rc_stick_img_0" android:src="@drawable/setting_ui_rc_stick_up_normal" style="@style/setting_ui_rc_stick_img" />
                    <TextView android:id="@id/setting_ui_rc_stick_txt_0" android:text="0" style="@style/setting_ui_rc_stick_txt" />
                    <ImageView android:id="@id/setting_ui_rc_stick_img_1" android:src="@drawable/setting_ui_rc_stick_up_normal" style="@style/setting_ui_rc_stick_img" />
                    <TextView android:id="@id/setting_ui_rc_stick_txt_1" android:text="1" style="@style/setting_ui_rc_stick_txt" />
                </LinearLayout>
                <LinearLayout style="@style/setting_ui_rc_stick_ly">
                    <ImageView android:id="@id/setting_ui_rc_stick_img_2" android:src="@drawable/setting_ui_rc_stick_up_normal" style="@style/setting_ui_rc_stick_img" />
                    <TextView android:id="@id/setting_ui_rc_stick_txt_2" android:text="2" style="@style/setting_ui_rc_stick_txt" />
                    <ImageView android:id="@id/setting_ui_rc_stick_img_3" android:src="@drawable/setting_ui_rc_stick_up_normal" style="@style/setting_ui_rc_stick_img" />
                    <TextView android:id="@id/setting_ui_rc_stick_txt_3" android:text="3" style="@style/setting_ui_rc_stick_txt" />
                </LinearLayout>
                <LinearLayout style="@style/setting_ui_rc_stick_ly">
                    <ImageView android:id="@id/setting_ui_rc_stick_img_4" android:src="@drawable/setting_ui_rc_stick_up_normal" style="@style/setting_ui_rc_stick_img" />
                    <TextView android:id="@id/setting_ui_rc_stick_txt_4" android:text="4" style="@style/setting_ui_rc_stick_txt" />
                    <ImageView android:id="@id/setting_ui_rc_stick_img_5" android:src="@drawable/setting_ui_rc_stick_up_normal" style="@style/setting_ui_rc_stick_img" />
                    <TextView android:id="@id/setting_ui_rc_stick_txt_5" android:text="5" style="@style/setting_ui_rc_stick_txt" />
                </LinearLayout>
                <LinearLayout android:id="@id/setting_ui_rc_stick_ly_3" style="@style/setting_ui_rc_stick_ly">
                    <ImageView android:id="@id/setting_ui_rc_stick_img_6" android:src="@drawable/setting_ui_rc_stick_up_normal" style="@style/setting_ui_rc_stick_img" />
                    <TextView android:id="@id/setting_ui_rc_stick_txt_6" android:text="6" style="@style/setting_ui_rc_stick_txt" />
                    <ImageView android:id="@id/setting_ui_rc_stick_img_7" android:src="@drawable/setting_ui_rc_stick_up_normal" style="@style/setting_ui_rc_stick_img" />
                    <TextView android:id="@id/setting_ui_rc_stick_txt_7" android:text="7" style="@style/setting_ui_rc_stick_txt" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:orientation="horizontal" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/dp_10">
                <FrameLayout android:layout_width="0.0dip" android:layout_height="wrap_content" android:layout_weight="1.0">
                    <RelativeLayout android:layout_gravity="center" android:layout_width="@dimen/dp_158" android:layout_height="@dimen/dp_150">
                        <ImageView android:id="@id/setting_ui_rc_stick_0" android:layout_alignParentLeft="true" android:layout_centerVertical="true" style="@style/setting_ui_rc_stick_img" />
                        <ImageView android:id="@id/setting_ui_rc_stick_1" android:layout_alignParentRight="true" android:layout_centerVertical="true" style="@style/setting_ui_rc_stick_img" />
                        <ImageView android:id="@id/setting_ui_rc_stick_2" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" style="@style/setting_ui_rc_stick_img" />
                        <ImageView android:id="@id/setting_ui_rc_stick_3" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" style="@style/setting_ui_rc_stick_img" />
                        <ImageView android:layout_width="@dimen/dp_58" android:layout_height="@dimen/dp_58" android:src="@drawable/setting_ui_rc_stick_circle" android:layout_centerInParent="true" />
                    </RelativeLayout>
                </FrameLayout>
                <FrameLayout android:layout_width="0.0dip" android:layout_height="wrap_content" android:layout_weight="1.0">
                    <RelativeLayout android:layout_gravity="center" android:layout_width="@dimen/dp_158" android:layout_height="@dimen/dp_150">
                        <ImageView android:id="@id/setting_ui_rc_stick_4" android:layout_alignParentLeft="true" android:layout_centerVertical="true" style="@style/setting_ui_rc_stick_img" />
                        <ImageView android:id="@id/setting_ui_rc_stick_5" android:layout_alignParentRight="true" android:layout_centerVertical="true" style="@style/setting_ui_rc_stick_img" />
                        <ImageView android:id="@id/setting_ui_rc_stick_6" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" style="@style/setting_ui_rc_stick_img" />
                        <ImageView android:id="@id/setting_ui_rc_stick_7" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" style="@style/setting_ui_rc_stick_img" />
                        <ImageView android:layout_width="@dimen/dp_58" android:layout_height="@dimen/dp_58" android:src="@drawable/setting_ui_rc_stick_circle" android:layout_centerInParent="true" />
                    </RelativeLayout>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
        <Button android:layout_gravity="center_horizontal" android:id="@id/setting_ui_rc_gimbal_speed" android:visibility="gone" android:text="@string/setting_ui_rc_gimbal_speed_desc" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" style="@style/setting_ui_item_btn" />
    </RelativeLayout>
</LinearLayout>