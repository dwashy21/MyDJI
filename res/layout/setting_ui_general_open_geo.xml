<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:orientation="vertical" style="@style/setting_ui_item"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:orientation="horizontal" style="@style/setting_ui_item">
        <TextView android:id="@id/setting_ui_geo_title" android:text="@string/general_open_geo" style="@style/setting_ui_item_title" />
        <Switch android:id="@id/setting_ui_item_switch" style="@style/setting_ui_item_switch" />
    </LinearLayout>
    <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/setting_ui_item_padding">
        <ImageView android:background="@drawable/setting_genneral_geo_desc_img" android:layout_width="wrap_content" android:layout_height="wrap_content" />
        <TextView android:layout_marginLeft="@dimen/setting_ui_item_padding" android:text="@string/general_open_geo_desc" style="@style/setting_ui_item_desc" />
    </LinearLayout>
</LinearLayout>