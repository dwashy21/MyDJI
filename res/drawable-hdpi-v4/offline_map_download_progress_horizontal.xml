<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="270.0" android:centerY="0.75" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="0.0dip" />
                <gradient android:startColor="@color/offline_map_hint_no_satellite_map_text_color" android:endColor="@color/offline_map_hint_no_satellite_map_text_color" android:angle="270.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff0061df" android:endColor="#ff0061df" android:angle="270.0" android:centerY="0.75" />
            </shape>
        </clip>
    </item>
</layer-list>